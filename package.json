{
  "name": "ibird-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for ibird.",
  "main": "index.js",
  "scripts": {
    "predev": "npm install",
    "dev": "nodemon --inspect src/index.js",
    "prestart": "NODE_ENV=production npm install --only=production",
    "start": "NODE_ENV=production node index.js",
    "build": "gulp",
    "postbuild": "npm run doc",
    "doc": "apidoc -i src/docs/ -o dist/assets/docs/",
    "test": "mocha -t 5000 -s 1000 --recursive test/**/*.js",
    "pdev": "sh bin/merge_master.sh",
    "ptest": "sh bin/merge_test.sh",
    "pprod": "sh bin/merge_prod.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yinfxs/ibird-boilerplate.git"
  },
  "author": "yinfxs <yinfxs@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yinfxs/ibird-boilerplate/issues"
  },
  "homepage": "https://github.com/yinfxs/ibird-boilerplate#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "ibird": "^1.0.0-alpha.12",
    "ibird-accounts": "^1.0.0-alpha.5",
    "ibird-i18n": "^1.0.0-alpha.10",
    "ibird-logger": "^1.0.0-alpha.7",
    "ibird-mongoose": "^1.0.0-alpha.15",
    "ibird-open": "^1.0.0-alpha.1",
    "ibird-utils": "^0.9.18",
    "koa-logger": "^3.1.0",
    "koa-session": "^5.5.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "boxen": "^1.3.0",
    "chalk": "^2.3.0",
    "clipboardy": "^1.2.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "ip": "^1.1.5",
    "nodemon": "^1.12.1"
  },
  "apidoc": {
    "name": "ibird-boilerplate",
    "version": "1.0.0",
    "description": "ibird API接口文档",
    "title": "ibird - API接口文档",
    "sampleUrl": "http://localhost:3000/api",
    "template": {
      "withCompare": false,
      "withGenerator": false
    }
  }
}
